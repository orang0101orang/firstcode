#include<stdio.h>

int main()
{
    int marks[4];
    marks[0]=100;
    marks[1]=99;
    marks[2]=100;
    marks[3]=98;
    printf("Marks of student 1 is %d\n",marks[0]);
    printf("Marks of student 2 is %d\n",marks[1]);
    printf("Marks of student 3 is %d\n",marks[2]);
    printf("Marks of student 4 is %d\n",marks[3]);
    return 0;
}
